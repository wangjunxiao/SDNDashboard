<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" 
		xmlns:mx="library://ns.adobe.com/flex/mx">
	<!-- host component -->
	<fx:Metadata>
		<![CDATA[
			[HostComponent("spark.components.SkinnableContainer")]
		]]>
	</fx:Metadata>
	
	<s:states>
		<s:State name="normal"/>
		<s:State name="disabled"/>
	</s:states>
	
	<s:Rect height="100%" width="100%">
		<s:fill>
			<s:LinearGradient>
				<mx:GradientEntry color="#608F9F"/> <!-- wathet blue 608F9F  Navy Blue 014D67 -->
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
	
	<s:Group id="contentGroup" left="10" top="1" bottom="1">
		<s:layout>
			<s:BasicLayout/>
		</s:layout>
	</s:Group>
	
</s:SparkSkin>
